<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.netvlops.nv.MainActivity"
    tools:showIn="@layout/activity_main">


    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/ivKegiatan"
        android:src="@drawable/sss"
        android:layout_marginTop="44dp"
        android:layout_below="@+id/ivJurusan"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/ivJurusan"
        android:src="@drawable/network"
        android:layout_marginTop="125dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/ivKegiatan"
        android:layout_alignStart="@+id/ivKegiatan" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/ivGuru"
        android:src="@drawable/aaa"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/ivJurusan"
        android:layout_alignStart="@+id/ivJurusan" />


</RelativeLayout>
